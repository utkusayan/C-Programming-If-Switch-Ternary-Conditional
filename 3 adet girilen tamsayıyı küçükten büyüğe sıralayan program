#include <stdio.h>
#include <stdlib.h>

int main()
{
    int yedek;
    int sayi1,sayi2,sayi3;
    printf("sayilari giriniz(3 adet):");
    scanf("%d%d%d",&sayi1,&sayi2,&sayi3);
    if(sayi1>sayi2)
    {
        yedek=sayi1;
        sayi1=sayi2;
        sayi2=yedek;
    }
    if(sayi2>sayi3)
    {
        yedek=sayi2;
        sayi2=sayi3;
        sayi3=yedek;
    }
    if(sayi1>sayi2)
    {
        yedek=sayi1;
        sayi1=sayi2;
        sayi2=yedek;
    }
    printf("Girilenlerin sirali hali=%d %d %d",sayi1,sayi2,sayi3);
    return 0;
}
